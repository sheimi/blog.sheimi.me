layout: nil
permalink: /feed/index.html
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Sheimi's Hive</title>
 <link href="http://blog.sheimi.me/atom.xml" rel="self"/>
 <link href="http://blog.sheimi.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.sheimi.me/</id>
 <author>
   <name>Sheimi</name>
   <email>sheimi.zhang@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://blog.sheimi.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blog.sheimi.me{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
